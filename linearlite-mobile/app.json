{
  "expo": {
    "name": "linearlite-mobile",
    "slug": "linearlite-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "linearlite",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.expodevsuccess.linearlitemobile",
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#FAFAFA",
        "dark": {
          "image": "./assets/images/splash-dark.png",
          "resizeMode": "cover",
          "backgroundColor": "#18181B"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.expodevsuccess.linearlitemobile",
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#FAFAFA",
        "dark": {
          "image": "./assets/images/splash-dark.png",
          "resizeMode": "cover",
          "backgroundColor": "#18181B"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-build-properties",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    },
    "extra": {
      "eas": {
        "projectId": "44ced8d2-79ed-4207-ad1d-1764585dc46c"
      }
    },
    "owner": "expo-dev-success",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/44ced8d2-79ed-4207-ad1d-1764585dc46c"
    }
  }
}
